    "@googlemaps/markerclusterer": "^2.0.11"
  },
  "peerDependencies": {
    "vue": "^2.7.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
